[*]
indent_style = tab
indent_size = 4
tab_width = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 80
quotes_type = double
spaces_around_operators = true
spaces_around_brackets = outside
wildcard_import_limit = 5
continuation_indent_size = 2

